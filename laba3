#include <iostream>
#include <cmath>
using namespace std;

double calculateY(double a, double b, double x) {
    double numerator = pow(1 + sqrt(a * x + b), 0.25); // корінь в четвертій степені
    double denominator = pow(sin(b * x), 2) + a; // sin^2(bx) + a
    return numerator / denominator;
}

